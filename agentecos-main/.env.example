# Application Configuration
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://localhost:5173

# Google Cloud Configuration
GCP_PROJECT_ID=agent-ecos
GCP_REGION=us-central1
GCP_KEY_FILE=agent-ecos-key.json

